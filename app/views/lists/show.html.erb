<h1><%= @list.name%></h1>

<ul>
<% @list.bookmarks.each do |bookmark|%>
<li>
<strong><%= bookmark.movie.title%></strong>
<%= bookmark.comment%>
</li>
<%= link_to "Delete Bookmark",
             bookmark_path(bookmark),
             method: :delete,
             data: { confirm:'Are you sure?' }%>

<% end%>
</ul>
<%= link_to "Add a bookMark" , new_list_bookmark_path(@list) %>
